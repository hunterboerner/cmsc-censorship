  shorttitle = {Wireless Mesh Networks},
  author = {Akyildiz, Ian F. and Wang, Xudong and Wang, Weilin},
  date = {2005-03-15},
  journaltitle = {Computer Networks},
  shortjournal = {Computer Networks},
  volume = {47},
